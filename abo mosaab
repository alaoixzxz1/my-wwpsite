   <script>
       document.addEventListener('DOMContentLoaded', function () {
           const toggles = document.querySelectorAll('.person.has-children');

           // Function to open all parent branches for a given element
           function openParents(element) {
               let current = element;
               while (current && current.id !== 'dhawi-children') {
                   const parentUl = current.closest('ul');
                   if (parentUl && parentUl.id) {
                        if(parentUl.classList.contains('hidden')) {
                           parentUl.classList.remove('hidden');
                           const parentToggle = document.querySelector(`[data-target="#${parentUl.id}"]`);
                           if (parentToggle) {
                               parentToggle.classList.remove('collapsed');
                               const icon = parentToggle.querySelector('.toggle-icon');
                               if (icon) {
                                   icon.style.transform = 'rotate(0deg)';
                               }
                           }
                       }
                   }
                   current = parentUl ? parentUl.parentElement : null;
               }
           }
           
           // Initially open the first level (children of Dhawi)
           const dhawiChildren = document.getElementById('dhawi-children');
           const dhawiToggle = document.querySelector('[data-target="#dhawi-children"]');
           if(dhawiChildren && dhawiToggle) {
               dhawiChildren.classList.remove('hidden');
               dhawiToggle.classList.remove('collapsed');
               const icon = dhawiToggle.querySelector('.toggle-icon');
               if (icon) {
                   icon.style.transform = 'rotate(0deg)';
               }
           }


           toggles.forEach(toggle => {
               // Set initial state for icons (collapsed or expanded)
               const targetId = toggle.dataset.target;
               if(targetId) {
                   const targetElement = document.getElementById(targetId.substring(1));
                    if (targetElement && targetElement.classList.contains('hidden')) {
                       toggle.classList.add('collapsed');
                       const icon = toggle.querySelector('.toggle-icon');
                       if (icon) {
                           icon.style.transform = 'rotate(-90deg)';
                       }
                   }
               }

               // Add click listener to toggle branches
               toggle.addEventListener('click', function (event) {
                   event.stopPropagation(); // Prevent event bubbling to parent toggles

                   const targetId = this.dataset.target;
                   if (!targetId) return;

                   const targetElement = document.getElementById(targetId.substring(1));
                   if (!targetElement) return;

                   // Toggle visibility of the children list
                   targetElement.classList.toggle('hidden');
                   
                   // Toggle the 'collapsed' class and rotate the icon
                   this.classList.toggle('collapsed');
                   const currentIcon = this.querySelector('.toggle-icon');
                   if (this.classList.contains('collapsed')) {
                       currentIcon.style.transform = 'rotate(-90deg)';
                   } else {
                       currentIcon.style.transform = 'rotate(0deg)';
                   }
               });
           });
       });
   </script>

</body>
</html>
